pipeline {
    agent any
    stages {
        stage('Linux example') {
            options {
                // Timeout counter starts BEFORE agent is allocated
                timeout(time: 1, unit: 'SECONDS')
            }
            steps {
                echo 'Hello World'
            }
        }
        stage('Windows example') {
            agent {
                node {
                    label 'windows10'
                }
            }
            steps {
                script {
                    bat 'scripts/s1.bat'
                }
            }
        } 
    }
}